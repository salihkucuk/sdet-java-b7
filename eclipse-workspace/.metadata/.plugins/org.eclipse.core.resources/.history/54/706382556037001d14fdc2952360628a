package day08;

public class SpecAssignment {
	public static void main(String[] args) {
		
		int number = 100;
		System.out.println(number);
		
		//increase value of number by 10
		number = number+10;
		System.out.println(number);
		
		number+=10;
		System.out.println(number);
		
		number=number-20;
		System.out.println(number);
		
		
		
		
	}

}
