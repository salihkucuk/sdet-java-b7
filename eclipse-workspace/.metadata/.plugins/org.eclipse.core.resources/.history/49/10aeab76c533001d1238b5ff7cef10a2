package day05;

public class Swap {
	public static void main(String[] args) {
		String sky="yellow";
		String sun="blue";
		sky="blue";
		sun="yellow";
		
		//TODO swap values of these two variables without direct reassignment.
		
		System.out.println("Sky is "+ sky);
		System.out.println("Sun is "+ sun);
		
	}

}
